@model IEnumerable<MyBlog.Models.Logger>

@{
    ViewBag.Title = "博客访问日志";
    Layout = "~/Views/Shared/_Layout_Admin.cshtml";
}

<h2>博客访问日志</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IpAdrress)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Url)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BlogId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VisitedTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Counter)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Accessor.NickName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IpAdrress)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Url)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Blog.BlogTitle)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VisitedTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Counter)
        </td>
    </tr>
}

</table>
